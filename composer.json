{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://gitlab.paysera.net/dzmitry.bitsiutski/roadrunner-bundle"
        },
        {
            "type": "composer",
            "url": "https://private-repo-repman.paysera.net"
        },
        {
            "type": "composer",
            "url": "https://repo-repman.paysera.net"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "dzmitry.bitsiutski/roadrunner-bundle": "^0.0.69",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "google/protobuf": "^3.21",
        "grpc/grpc": "^1.57",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.7",
        "spiral/roadrunner": "^v2023.3.12",
        "spiral/roadrunner-grpc": "^3.3",
        "spiral/roadrunner-http": "^3.5",
        "spiral/roadrunner-jobs": "^4.4",
        "spiral/roadrunner-cli": "^2.6",
        "spiral/roadrunner-metrics": "^3.2",
        "spiral/roadrunner-kv": "^4.2",
        "sentry/sentry": "^4.8",
        "symfony/asset": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/filesystem": "6.4.*",
        "symfony/flex": "^1.17|^2",
        "symfony/framework-bundle": "6.4.*",
        "symfony/process": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/yaml": "6.4.*",
        "symfony/config": "6.4.*",
        "symfony/dependency-injection": "^6.0 || ^7.0",
        "symfony/http-kernel": "6.4.*",
        "symfony/http-foundation":"^6.2 || ^7.0",
        "symfony/error-handler":"^6.1|^7.0",
        "symfony/security-core": "^6.2|^7.0",
        "symfony/routing": "^6.4|^7.0",
        "symfony/security-http":"^6.3.6|^7.0",
        "symfony/cache": "^6.0|^7.0",
        "symfony/twig-bundle": "^6.0 || ^7.0",
        "sentry/sentry-symfony": "^5.0",
        "guzzlehttp/guzzle": "^7.8",
        "temporal/sdk": "^2.10",
        "nesbot/carbon": "^3.5"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {

    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/phpstan": "^1.8",
        "symfony/maker-bundle": "^1.45"
    }
}
